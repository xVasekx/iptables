[options]
  UseSyslog
  interface = {{interface}}

[SSH]
  sequence = {{port_sequence}}
  tcpflags = syn
  seq_timeout = {{seq_timeout}}
  start_command = /sbin/iptables -I INPUT 1 -s %IP% -p tcp --dport {{secure_ports}} -j ACCEPT
  cmd_timeout = {{cmd_timeout}}
  stop_command = /sbin/iptables -D INPUT -s %IP% -p tcp --dport {{secure_ports}} -j ACCEPT
